{
  "name": "waterwaves",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "author": "linhong",
  "main": "lib/waterwaves.umd.min.js",
  "keywords": [
    "water",
    "waterwaves",
    "vue",
    "components"
  ],
  "files": [
    "lib",
    "example"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/example/main.js",
    "build": "vue-cli-service build --target lib --name waterwaves --dest lib src/lib/index.js && vue-cli-service build --dest example src/example/main.js",
    "lib": "vue-cli-service build --target lib --name waterwaves --dest lib src/lib/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acdseen/waterWaves.git"
  },
  "bugs": {
    "url": "https://github.com/acdseen/waterWaves/issues"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
